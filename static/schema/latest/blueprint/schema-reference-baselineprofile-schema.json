{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "$id": "https://hamlet.io/schema/latest/blueprint/reference-baselineprofile-schema.json",
  "definitions": {
    "BaselineProfile": {
      "type": "object",
      "patternProperties": {
        "^[A-Za-z_][A-Za-z0-9_]*$": {
          "properties": {
            "OpsData": {
              "type": "string"
            },
            "AppData": {
              "type": "string"
            },
            "Encryption": {
              "type": "string"
            },
            "SSHKey": {
              "type": "string"
            },
            "CDNOriginKey": {
              "type": "string"
            }
          },
          "additionalProperties": false,
          "required": [
            "OpsData",
            "AppData",
            "Encryption",
            "SSHKey",
            "CDNOriginKey"
          ]
        }
      }
    }
  }
}
